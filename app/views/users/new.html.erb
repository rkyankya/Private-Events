<h1> Sign Up </h1>
<hr>
<div>
    <%= form_for @users do |f| %>
    <%= f.label :name, "string" %>
    <%= <%= f.text_area :name %>

    <%= f.label :email, "email" %>
    <%= <%= f.text_area :email %>

    <%= f.label :password, "password" %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation, "password" %>
    <% <%= f.password_field :password_confirmation %>%>

    <% end %>
</div>